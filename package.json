{
  "name": "gatsby-dev-blog-starter",
  "description": "Gatsby starter to get up and running with personal programming blog",
  "version": "0.1.0",
  "author": "Vladimir Skok <skok@vova.io>",
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.30.1",
    "gatsby": "^2.3.21",
    "gatsby-cli": "^2.5.12",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-offline": "^2.0.22",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-twitter": "^2.0.13",
    "gatsby-prismjs-dracula": "^0.1.15",
    "gatsby-remark-autolink-headers": "^2.0.13",
    "gatsby-remark-code-buttons": "^2.0.1",
    "gatsby-remark-code-titles": "^1.1.0-rc.0",
    "gatsby-remark-emoji-unicode": "^0.1.0",
    "gatsby-remark-images": "^3.0.3",
    "gatsby-remark-prismjs": "^3.2.4",
    "gatsby-remark-responsive-iframe": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-remark": "^2.2.3",
    "gatsby-transformer-sharp": "^2.1.13",
    "node-sass": "^4.11.0",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-cookie": "^3.1.2",
    "react-dom": "^16.7.0",
    "react-headroom": "^2.2.4",
    "react-helmet": "^5.2.0",
    "react-particles-js": "^2.4.2",
    "rehype-format": "^2.3.1",
    "rehype-prism-template": "^0.4.0",
    "rehype-raw": "^4.0.0",
    "rehype-sanitize": "^2.0.2",
    "rehype-stringify": "^5.0.0",
    "remark-breaks": "^1.0.2",
    "remark-emoji": "^2.0.2",
    "remark-github": "^7.0.6",
    "remark-parse": "^6.0.3",
    "remark-rehype": "^4.0.0",
    "unified": "^7.1.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build:functions && npm run build:gatsby",
    "build:gatsby": "gatsby build",
    "build:functions": "netlify-lambda build src/functions",
    "develop:functions": "netlify-lambda serve src/functions",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "commit": "git-cz",
    "release": "standard-version",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "commitizen": "^3.0.5",
    "cross-env": "5.2.0",
    "cz-customizable": "^5.3.0",
    "netlify-cli": "2.11.23",
    "netlify-lambda": "^1.4.13",
    "prettier": "^1.16.4",
    "standard-version": "^4.4.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizenrc"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamskok/gatsby-dev-blog-starter"
  },
  "bugs": {
    "url": "https://github.com/iamskok/gatsby-dev-blog-starter/issues"
  }
}
