{
  "name": "gatsby-dev-blog-starter",
  "description": "Gatsby starter to get up and running with personal web development blog",
  "version": "0.1.1",
  "author": "Vladimir Skok <skok@vova.io>",
  "dependencies": {
    "@material-ui/core": "^3.7.1",
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-responsive-iframe": "^2.0.8",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.8",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "commit": "git-cz",
    "release": "standard-version",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "commitizen": "^3.0.5",
    "cz-customizable": "^5.3.0",
    "prettier": "^1.15.2",
    "standard-version": "^4.4.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizenrc"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamskok/gatsby-dev-blog-starter"
  },
  "bugs": {
    "url": "https://github.com/iamskok/gatsby-dev-blog-starter/issues"
  }
}
